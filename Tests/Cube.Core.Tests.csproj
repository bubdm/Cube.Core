<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Version>1.15.0</Version>
        <Authors>CubeSoft</Authors>
        <Company>CubeSoft, Inc.</Company>
        <Product>Cube.Tests</Product>
        <Description>NUnit framework を用いて Cube.Core プロジェクトをテストします。</Description>
        <Copyright>Copyright © 2010 CubeSoft, Inc.</Copyright>
        <AssemblyTitle>Cube.Core UnitTest</AssemblyTitle>
        <RootNamespace>Cube.Tests</RootNamespace>
        <AssemblyName>Cube.Core.Tests</AssemblyName>
        <TargetFramework>net45</TargetFramework>
    </PropertyGroup>
    <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
        <OutputPath>bin\$(Platform)\$(Configuration)\</OutputPath>
        <DocumentationFile>bin\Any CPU\$(Configuration)\$(TargetFramework)\Cube.Core.Tests.xml</DocumentationFile>
        <DefineConstants>DEBUG;TRACE</DefineConstants>
    </PropertyGroup>
    <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
        <OutputPath>bin\Any CPU\$(Configuration)\</OutputPath>
        <DocumentationFile>bin\Any CPU\$(Configuration)\$(TargetFramework)\Cube.Core.Tests.xml</DocumentationFile>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\Libraries\Cube.Core.csproj" />
        <PackageReference Include="log4net" Version="2.0.8" />
        <PackageReference Include="NUnit" Version="3.11.0" />
        <Reference Include="System" />
        <Reference Include="System.Runtime.Serialization" />
        <Reference Include="System.XML" />
        <Reference Include="System.Xml.Linq" />
    </ItemGroup>
    <ItemGroup>
        <None Include="App.config" />
        <None Include="Examples\Sample.opml" CopyToOutputDirectory="Always" />
        <None Include="Examples\Sample.rss" CopyToOutputDirectory="Always" />
        <None Include="Examples\Sample.txt" CopyToOutputDirectory="Always" />
        <None Include="Examples\Settings.ja.json" CopyToOutputDirectory="Always" />
        <None Include="Examples\Settings.ja.xml" CopyToOutputDirectory="Always" />
        <None Include="Examples\Settings.json" CopyToOutputDirectory="Always" />
        <None Include="Examples\Settings.xml" CopyToOutputDirectory="Always" />
    </ItemGroup>
</Project>